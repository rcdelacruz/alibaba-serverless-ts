{
  "name": "serverless-aliyun-hello-world",
  "version": "1.0.0",
  "description": "Serverless hello world sample for Alibaba Cloud Functions",
  "main": "index.js",
  "scripts": {
    "test": "echo \"No tests specified\" && exit 0",
    "deploy": "serverless deploy",
    "deploy:assisted": "chmod +x ./deploy.sh && ./deploy.sh",
    "install-plugin": "npm install --save-dev github:aliyun/serverless-aliyun-function-compute",
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/local.ts",
    "start:local": "node lib/src/local.js",
    "remove": "serverless remove"
  },
  "keywords": [
    "serverless",
    "alibaba",
    "aliyun"
  ],
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/node": "^18.15.0",
    "serverless": "^3.28.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.5"
  }
}