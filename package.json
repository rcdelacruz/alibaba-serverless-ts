{
  "name": "alibaba-nodejs-ts",
  "version": "1.0.0",
  "description": "Serverless TypeScript sample for Alibaba Cloud Functions",
  "main": "handler.js",
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn src/local.ts",
    "start": "node lib/local.js",
    "test": "echo \"No tests specified\" && exit 0",
    "deploy": "npm run build && serverless deploy"
  },
  "keywords": [
    "serverless",
    "typescript",
    "alibaba",
    "aliyun"
  ],
  "dependencies": {
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.5",
    "@types/express": "^4.17.21",
    "@types/node": "^18.15.0",
    "serverless": "^3.28.1",
    "serverless-aliyun-function-compute": "^3.2.11",
    "serverless-typescript": "^1.1.9",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
